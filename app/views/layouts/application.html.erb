<!DOCTYPE html>
<html>
  <head>
    <title>Advertising Portal</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg navbar-light nav_bar_custom">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="/"><h1>Advertising Portal</h1></a>
        </div>
        <ul class="nav navbar-nav navbar-right">
          <li><a  class="nav-link" href="/login"></span >Login</a></li>
          <li><a class="nav-link" href="/register"></span >Register</a></li>
        </ul>
      </div>
    </nav>
    <%= yield %>
  </body>
</html>
